{
  "name": "mega-client",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:core": "turbo run build --filter=@mega-dao/core",
    "build:client": "turbo run build --filter=@mega-dao/client",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "^1.9.3"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
